//Tinh tong so lan xuat hien cua string a trong string b
#include <stdio.h>
#include <string.h>
void sum()
{
	int cnt=0;
	char a[100];
	char b[100];
	printf("Nhap xau a: ");
	gets(a);
	printf("Nhap xau b: ");
	gets(b);
	for (int i = 0; i <= strlen(b) - strlen(a); i++) 
        for (int j = 0; j < strlen(a); j++) 
		{
            if (b[i+j] != a[j]) 
			{
                break;
            }
        } cnt++;
    printf("%d", cnt);
}
int main()
{
	sum();
}
