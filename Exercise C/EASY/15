//tinh n giai thua
#include <stdio.h>
void factorial()
{
	int num=1;
	int k;
	printf ("Nhap so bat ky: ");
	scanf("%d", &k);
	if(num<0)
	{ 
	   printf("Vui long nhap lai so duong: ");
	   scanf ("%d", &num);
	}
	for (int i=1;i<k+1;i++)
	{
		num=i*num;
	}
	printf ("Giai thua cua so do bang %d", num);
}
int main()
{
	factorial();
}
