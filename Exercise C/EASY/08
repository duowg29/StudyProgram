//viet ham kiem tra so nguyen to
#include <stdio.h>
void check()
{
	int num;
    printf ("Nhap so: ");
    scanf ("%d", &num);
    for (int i=2;i<num;i++)
	{
		if(num%i!=0)
		{
			printf ("La so nguyen to");
		}else
	     printf ("Khong phai so nguyen to");
	     break;
	}
}
int main()
{
	int num;
	check();
}
