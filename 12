//viet ham random tra ve mot gia tri trong khoang tu -a den a
#include <stdio.h>
#include <stdlib.h>
int random_num(int a)
{
	return rand() % (2 * a ) - a;
}
int main()
{
	int a;
	printf ("Nhap 1 so: ");
	scanf("%d", &a);
	int random_val= random_num(a);
    printf ("Gia tri ngau nhien tu -%d den %d la: %d",a, a, random_val);
}
